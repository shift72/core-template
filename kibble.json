{
  "name": "core-template",
  "version": "0.0.1",
  "siteUrl": "https://staging-letterboxd.shift72.com",
  "builderVersion": "0.17.0",
  "defaultLanguage": "en",
  "languages": {
    "en": {
      "code": "en_AU",
      "name": "English"
    }
  },
  "siteRootPath": "site",
  "liveReload": {
    "ignoredPaths": [
      "styles",
      "js"
    ]
  },
  "routes": [
    {
      "name": "filmItem",
      "urlPath": "/film/:slug/",
      "templatePath": "templates/film/item.jet",
      "partialUrlPath": "/partials/film/:filmID.html",
      "partialTemplatePath": "templates/film/partial.jet",
      "datasource": "Film",
      "pageSize": 0
    }
  ],
  "coreTemplateVersion": "1.8.0"
}