{{block linkoverrides()}}
  <script type="text/javascript">
    document.addEventListener('s72loaded', function(event) {
      var app = event.detail.app;
      
      app.shoppingProcessService.setTermsAndConditionsUrl('{{ routeToPath("/page/terms-and-conditions/") }}');
      app.shoppingProcessService.setLibraryUrl('{{ routeToPath("/library.html") }}');
    });
  </script>
{{end}}