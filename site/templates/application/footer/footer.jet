{{import "./brand.jet"}}
{{import "./info.jet"}}
{{import "./nav.jet"}}
{{import "./app-badges.jet"}}
{{import "./sponsor-banner.jet"}}

{{block footer()}}

  {{yield appBadges()}}

  {{yield sponsorBanner()}}

  {*{ID is used for skip links.}*}
  <footer id="footer">
    {{yield footerBrand()}}

    {{yield footerNav(navLinks=site.Navigation.Footer)}}

    {{ yield footerSocialMediaButtons() }}

    {{yield footerInfo()}}
  </footer>
{{end}}
