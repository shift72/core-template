{{extends "templates/application.jet"}}
{{block head()}}
  {{yield seo() page}}
{{end}}
{{block body()}}
<main class="page content-page" role="main">
  <div class="page-header">
    <h1>{{ page.Title }}</h1>
  </div>
  {{if len(page.Content)}}
  <div class="content-page-content">
    {{page.Content | raw}}
  </div>
  {{end}}
</main>
{{end}}