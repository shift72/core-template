{{import "./carousel.jet"}}
{{import "./slider.jet"}}
{{import "./list.jet"}}

{{block pageCollection(showTitle=true)}}
  {{if .Layout == "carousel"}}
    {{yield pageCollectionCarousel()}}
  {{else if .Layout == "slider"}}
    {{yield pageCollectionSlider()}}
  {{else}}
    {{yield pageCollectionList(showTitle=showTitle)}}
  {{end}}
{{end}}