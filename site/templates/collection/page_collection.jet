{{import "templates/carousel/carousel.jet"}}
{{import "./slider.jet"}}
{{import "./list.jet"}}

{{block pageCollection()}}
	{{if .Layout == "carousel"}}
		{{yield carousel()}}
	{{else if .Layout == "slider"}}
		{{yield pageCollectionSlider()}}
	{{else}}
		{{yield pageCollectionList()}}
	{{end}}
{{end}}